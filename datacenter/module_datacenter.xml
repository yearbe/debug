      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="datacenter.module.test.sourcepath">
    <dirset dir="${module.datacenter.basedir}">
      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.datacenter" depends="compile.module.datacenter.production,compile.module.datacenter.tests" description="Compile module datacenter"/>
  
  <target name="compile.module.datacenter.production" depends="register.custom.compilers" description="Compile module datacenter; production classes">
    <mkdir dir="${datacenter.output.dir}"/>
    <javac2 destdir="${datacenter.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.datacenter}/javac">
      <compilerarg line="${compiler.args.datacenter}"/>
      <bootclasspath refid="datacenter.module.bootclasspath"/>
      <classpath refid="datacenter.module.production.classpath"/>
      <src refid="datacenter.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.datacenter"/>
    </javac2>
    
    <copy todir="${datacenter.output.dir}">
      <fileset dir="${module.datacenter.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.datacenter.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.datacenter.tests" depends="register.custom.compilers,compile.module.datacenter.production" description="compile module datacenter; test classes" unless="skip.tests">
    <mkdir dir="${datacenter.testoutput.dir}"/>
    <javac2 destdir="${datacenter.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.datacenter}/javac">
      <compilerarg line="${compiler.args.datacenter}"/>
      <bootclasspath refid="datacenter.module.bootclasspath"/>
      <classpath refid="datacenter.module.classpath"/>
      <src refid="datacenter.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.datacenter"/>
    </javac2>
    
    <copy todir="${datacenter.testoutput.dir}">
      <fileset dir="${module.datacenter.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.datacenter" description="cleanup module">
    <delete dir="${datacenter.output.dir}"/>
    <delete dir="${datacenter.testoutput.dir}"/>
  </target>
</project>